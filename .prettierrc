{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": false,
  "arrowParens": "always",
  "proseWrap": "always",
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "strict",
  "singleAttributePerLine": false,
  "quoteProps": "consistent",
  "embeddedLanguageFormatting": "auto"
}
